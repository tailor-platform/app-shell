{
  "name": "@tailor-platform/app-shell-vite-plugin",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tailor-platform/app-shell",
    "directory": "packages/vite-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./parser": {
      "types": "./dist/parser.d.mts",
      "default": "./dist/parser.mjs"
    }
  },
  "files": [
