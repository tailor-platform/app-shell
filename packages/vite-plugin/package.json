    "type-check": "tsc --incremental"
  },
  "peerDependencies": {
    "vite": ">= 5"
  },
  "dependencies": {
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "^25",
    "tsdown": "^0.20.3",
    "typescript": "^5",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
